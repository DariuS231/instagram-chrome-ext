{
  "manifest_version": 2,

  "name": "Chromestagram",
  "description": "Instagram feed in your Google Chrome",
  "version": "0.1",
  "icons": { 
    "128": "128_icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "identity"
  ],

  "oauth2": {
    "client_id": "55c4e90c0b3a41fab5b5c18c961ba3a5",
    "scopes": [
      "*://*.instagram.com/*"
    ]
  }
}
