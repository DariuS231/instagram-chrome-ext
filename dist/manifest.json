{
  "manifest_version": 2,

  "name": "Chromestagram",
  "description": "Instagram feed in your Google Chrome",
  "version": "0.1",
  "icons": { 
    "128": "128_icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
